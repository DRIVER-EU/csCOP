{
    "clientId": "csCOP-A",
    "kafkaHost": "localhost:3501",
    "schemaRegistry": "http://localhost:3502",
    "wrapUnions": "auto",
    "heartbeatInterval": 10000,
    "fetchAllSchemas": false,
    "fetchAllVersions": false,
    "fromOffset": true,
    "autoRegisterSchemas": true,
    "schemaFolder": "./data/schemas",
    "consume": [{
            "topic": "system_heartbeat",
            "offset": -1
        },
        {
            "topic": "cap"
        },
        {
            "topic": "css-demo-geojson"
        },
        {
            "topic": "teamcommunication"
        },
        {
            "topic": "standard_geojson_sim_item"
        },
        {
            "topic": "standard_geojson_sim_unit"
        },
        {
            "topic": "standard_geojson_sim_unitgroup"
        },
        {
            "topic": "standard_geojson_sim_station"
        }
    ],
    "produce": ["cap", "teamcommunication", "css-demo-geojson", "standard_geojson_sim_unit", "standard_geojson_sim_item", "standard_geojson_sim_unitgroup", "standard_geojson_sim_station"]
}
